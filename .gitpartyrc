gitparty:
  key: G
  color: bgBlueBright
  matches:
  - \**/gitparty.js
lint:
  key: L
  color: bgMagenta
  matches:
  - \**/.eslintrc
tests:
  key: T
  color: bgRed
  matches:
  - \**/*.spec.js
invisibles:
  key: R
  color: bgRedBright
  matches:
  - \**/.gitpartyrc
  - \**/.gitignore
  - \**/.npmignore
config:
  key: C
  color: bgCyan
  matches:
  - \**/package.json
  - \**/rollup/*
  - \**/webpack*
  - \**/^.*
dependencies:
  key: D
  color: bgYellow
  matches:
  - \**/package.json
  - \**/yarn.lock
hope:
  key: H
  color: bgBrightRed
  filter:
  - subject:should~
  - subject:fix~
  - subject:work~
collapseAuthors: false
